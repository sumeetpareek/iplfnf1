<?xml version="1.0" encoding="utf-8" ?><Module>
	<ModulePrefs title="T20WC 2009 Facts"
               title_url="http://iplfnf1.appspot.com"
               description="T20WC 2009 Facts.. very very good"
               author="Kaka"
               author_email="positivecharge@gmail.com"
               scaling="true"
               scrolling="false"
               singleton="false" > 
		<Require feature="opensocial-0.8"/>
		<Require feature="views" />
		<Require feature="dynamic-height"/>
		<Require feature="analytics"/>
		
		<Optional feature="maximize" />
    	<Optional feature="flash" />
    	<Optional feature="setprefs" />
    	
		<Optional feature="content-rewrite">
			<Param name="include-urls">.*</Param>
  			<Param name="exclude-urls"></Param>
  			<Param name="include-tags">img,script,embed,link,style</Param>
		</Optional>
			</ModulePrefs>
	
	<Content type="html" view="profile, canvas">

    <![CDATA[
    
		<link rel="stylesheet" type="text/css" href="wick.css" />
		<link rel="stylesheet" type="text/css" href="iplfnf.css" />
    <script src="http://iplfnf1.appspot.com/static/jquery.js" type="text/javascript"></script>
    <script src="http://iplfnf1.appspot.com/static/jquery.livequery.js" type="text/javascript"></script>
    <script src="http://iplfnf1.appspot.com/static/wick.js" type="text/javascript"></script>
    <script src="http://iplfnf1.appspot.com/static/script15.js" type="text/javascript"></script>
    
    <script>
    gadgets.util.registerOnLoadHandler(init);
    function init() {
      showLoading($("#fnf-container"),"Aabra Ka Daabra..!! ");
      registerFunctions();
      startGettingData();
    }
    </script>
    
		<div id="fnf-wp">
			
			<div id="fnf-header">
				<div class="fnf-tab-bar">
					<ul class="fnf-tab-menu">
						<li class="active">
							<a href="/info/opensocial/type/orkut">Facts</a>
						</li>
						<li class="">
							<a href="/info/opensocial/type/gmail">Fantasy</a>
						</li>
					</ul>
					<div class="fnf-tab-bar-bottom"></div>
				</div><!-- .fnf-tab-bar -->
			</div><!-- #fnf-header -->
			
			<div id="fnf-container">

				<div id="fact-container" style="display:none;">
					<div id="fact-tabs">
						<div class="fact-tab-bar-top"></div>
							<ul class="fact-tab-menu">
								<li class="">
									<a href="#" id="most-liked">Most Liked</a>
								</li>
								<li class="">
									<a href="#" id="most-disliked">Most Disliked</a>
								</li>
								<li class="">
									<a href="#" id="latest" class="active">Latest</a>
								</li>
								<li class="">
									<a href="#" id="user-added">Added by You</a>
								</li>
								<li class="">
									<a href="#" id="friend-added">Added by Friends</a>
								</li>
								<li class="">
									<a href="#" id="user-liked">Liked by You</a>
								</li>
								<li class="">
									<a href="#" id="user-disliked">Disliked by You</a>
								</li>
							</ul>
					</div><!-- #fact-tab-bar -->
	
					<div id="fact-add" class="collapsed">
						<div id="bold-add"><strong>Be Bold!</strong> Click here to share IPL facts with friends.</div>
					<div id='fact-add-wp' style="display:none;">
						<div class="uimg"></div>
						<form id='fact-add-form' action='#' method='post'>
						<textarea rows="5" cols="25" id="fact-add-content" name="fact-content"/></textarea>
						<div id='fact-add-tags'>
						<div id="all_player_coll">Players:<input id="wickPlayer" class="wickEnabled wickPlayer" type="text" size="30" /></div>
						<div id="all_club_coll">Clubs:<input id="wickClub" class="wickEnabled wickClub" type="text" size="30" /></div>
						<div class="cb"></div>
						<input id="fact-submit" type="submit" value="submit your IPL Fact!">
						<input id="fact-reset" type="reset" value="reset">
						<input id="fact-cancel" type="reset" value="cancel">
						<div class="cb"></div>
						<div class="fact-submit-msg-wp" style="display:none;">
							<div class="msg-y"></div>
							<div class="msg-r">Please enter some fact and atleast one associated tag in Player and/or Club field.<br/>(You cannot start bowling without specifing bowling style to the umpire !!?!?)</div>
						</div>
						</div>
						</form>
					</div>
					</div><!-- #fact-add -->
					
					<div id="fact-list-wp">
          <div class="cb"></div>
					</div><!-- #fact-list-wp -->
					
					<div id="fact-pager"><div id="fact-prev"> &lt; Previous </div><div id="fact-next"> Next &gt; </div><div class="cb"></div></div>
				</div><!-- #fact-container -->
			
			</div><!-- #fnf-container -->
		</div><!-- #fnf-wp -->

    ]]>
  </Content>
</Module>